<mvc:View
    controllerName="com.example.uiexperiment.controller.PostLogin"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:custom="com.example.uiexperiment.control"
    displayBlock="true"
    height="100%"
    busyIndicatorDelay="0"
>
    <Page title="Wizard Example">
        <content>
            <Wizard
                id="id.postLoginWizard"
                renderMode="Page"
                finishButtonText="Finish"
                steps="{/}"
                
            >
                <steps>
                    <WizardStep title="{key}" />
                </steps>
            </Wizard>
        </content>
    </Page>
</mvc:View>